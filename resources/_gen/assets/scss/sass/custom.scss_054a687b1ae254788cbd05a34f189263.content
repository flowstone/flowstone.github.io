a{text-decoration:none}.post-a-unstyled{margin-left:10px;cursor:pointer}article h3{color:#0e8aeb;border-top-style:none;border-bottom-style:none;border-left-style:solid;border-right-style:none;border-top-width:1px;border-bottom-width:1px;border-left-width:5px;border-right-width:1px;border-top-color:#000;border-bottom-color:#000;border-left-color:#0e88eb;border-right-color:#000;border-top-left-radius:0;border-top-right-radius:0;border-bottom-left-radius:0;border-bottom-right-radius:0;padding-left:10px}article h4{color:#0e8aeb}article h5{color:#0e8aeb}.container blockquote:before{quotes:none}.container blockquote{padding:15px 10px;font-size:14px;line-height:18px;padding-right:15px;margin-left:0;color:#819198;border-left:6px solid #dce6f0;background:#f2f7fb;display:block}table{border-collapse:collapse;margin:0 auto;text-align:center;margin-bottom:10px}table td,table th{border:1px solid #cad9ea;color:#666;height:30px}table thead th{background-color:#cce8eb}table td{padding:0 30px}table tr:nth-child(odd){background:#fff}table tr:nth-child(even){background:#f5fafa}article strong{font-size:25px;color:#d40f0f;margin:5px;background-color:#ecf800}article em{font-size:20px;color:#00a6ff;margin:5px}article img{border:2px dashed #0e95d9;border-radius:10px;margin:10px}